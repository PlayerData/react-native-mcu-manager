{
  "name": "@playerdata/react-native-mcu-manager",
  "title": "React Native MCU Manager",
  "version": "0.0.1-development",
  "description": "React Native Wrappers for MCUMgr's Android / iOS client libraries",
  "source": "src/",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "open -a \"Xcode\" example/ios",
    "open:android": "open -a \"Android Studio\" example/android"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "mcumgr"
  ],
  "author": "PlayerData <dev@playerdata.com> (https://github.com/PlayerData)",
  "license": "MIT",
  "repository": "https://github.com/PlayerData/react-native-mcu-manager",
  "bugs": {
    "url": "https://github.com/PlayerData/react-native-mcu-manager/issues"
  },
  "homepage": "https://github.com/PlayerData/react-native-mcu-manager#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "expo": "~49.0.15",
    "expo-splash-screen": "~0.22.0",
    "expo-status-bar": "~1.7.0",
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "^18.0.25",
    "@types/uuid": "9.0.7",
    "commitlint": "18.4.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "expo-module-scripts": "^3.0.11",
    "expo-modules-core": "^1.5.11",
    "husky": "^8.0.0",
    "prettier": "^3.0.0",
    "semantic-release": "^22.0.0",
    "@babel/core": "^7.20.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
